<view><scroll-view class="page" scroll-y="{{true}}"><view class="cu-list menu-avatar card-menu margin-top"><view class="cu-item"><view class="cu-avatar round  lg" style="background-image:url(https://ossweb-img.qq.com/images/lol/web201310/skin/big10001.jpg);"></view><view class="content"><view class="text-white">嗯嗯嗯</view></view><view class="padding flex flex-wrap justify-between align-center text-color"><button class="cu-btn round bg-yellow shadow">刷新</button></view></view><view class="bg-white flex padding justify-between"><block wx:for="{{time_list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="text-lg padding"><view class="padding margin-bottom-sm">{{item.title}}</view><view class="text-bold">{{item.num}}</view></view></block></view></view><view class="margin-top-xl"><view class="menu card-menu  cu-list sm-border"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-item arrow" data-url="{{item.url}}" data-event-opts="{{[['tap',[['toChild',['$event']]]]]}}" bindtap="__e"><view class="content"><text class="{{['margin-right-sm','cuIcon-'+item.cuIcon,'text-'+item.color]}}"></text><text class="text-grey">{{item.title}}</text></view></view></block></view></view><view class="cu-list menu  card-menu margin-top sm-border"><view class="cu-item arrow"><button class="cu-btn content" open-type="contact"><text class="cuIcon-btn text-olive"></text><text class="text-grey">客服</text></button></view><view data-event-opts="{{[['tap',[['jump',['$event']]]]]}}" class="cu-item arrow" bindtap="__e"><view class="content"><text class="cuIcon-circlefill text-grey"></text><text class="text-grey">关于</text></view></view></view><view class="cu-tabbar-height"></view></scroll-view></view>